# Server
HOST=0.0.0.0
PORT=8000
LOG_LEVEL=info
ALLOWED_ORIGINS=*

# Feature flags
MCP_ENABLED=1
OPENAI_ASSISTANTS_ENABLED=0
# Show rich match UI blocks (Table + SkillsBadges + MatchBreakdown) in Copilot chat
CHAT_MATCH_UI=1
# Enforce real data only (no synthetic bootstraps or LLM geocoding fallbacks)
STRICT_REAL_DATA=1

# API security
API_KEY=change-me

# Database
MONGO_URI=mongodb://localhost:27017
DB_NAME=talent_match

# Optional: disable LLM-based geocoding (keep deterministic)
GEO_LLM_ENABLED=0
